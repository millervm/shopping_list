<%= form_for @item do |f| %>
    <%= f.hidden_field :list_id %>
    <%= f.hidden_field :user_id, value: @item.list.user.id %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <br></br>
    <%= f.label :description %>
    <br>
    <%= f.text_area :description %>
    <br></br>
    <%= f.label :urgent %>
    <%= f.check_box :urgent %>
    <br></br>
    <%= f.submit %>
<% end %>